TITLE   8086 Code Template (for EXE file)

       #MAKE_EXE#

DSEG    SEGMENT 'DATA'
table   db 1, 2, 3, 4, 5
min     db ?
DSEG    ENDS

SSEG    SEGMENT STACK   'STACK'
        DW      100h    DUP(?)
SSEG    ENDS

CSEG    SEGMENT 'CODE'

START   PROC    FAR
    PUSH    DS
    MOV     AX, 0
    PUSH    AX

    MOV     AX, DSEG
    MOV     DS, AX
    MOV     ES, AX

    ; Find minimum value
    MOV     CX, 5
    LEA     BX, table
    MOV     DL, [BX]
    MOV     min, DL

FIND_MIN:
    MOV     AL, [BX]
    CMP     AL, min
    JAE     NEXT
    MOV     min, AL
    
NEXT:
    INC     BX
    LOOP    FIND_MIN

    RET
START   ENDP

CSEG    ENDS 
        END    START
